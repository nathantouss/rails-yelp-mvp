<h1>New Review</h1>

<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :rating %>
  <%= f.input :content %>
  <!-- Add more input fields for other attributes as needed -->

  <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>

<%= link_to 'Back to Restaurants', restaurants_path, class: 'btn btn-secondary' %>
